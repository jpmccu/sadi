<?xml version="1.0"?>
<!DOCTYPE rdf:RDF [
  <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
  <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
  <!ENTITY owl "http://www.w3.org/2002/07/owl#">
  <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
  <!ENTITY benv "http://sadiframework.org/ontologies/predicates.owl#">
  <!ENTITY common "http://sadiframework.org/examples/common.owl#">
]>
<rdf:RDF
    xmlns:rdf="&rdf;"
    xmlns:rdfs="&rdfs;"
    xmlns:owl="&owl;"
    xmlns:xsd="&xsd;"
    xmlns:benv="&benv;">
    
  <owl:Ontology rdf:about=""/>
  
  <!-- in lieu of importing the whole ontology... -->
  <owl:DatatypeProperty rdf:about="&benv;hasName"/>
  <owl:DatatypeProperty rdf:about="&benv;hasDescription"/>
  <owl:DatatypeProperty rdf:about="&benv;belongsToOrganism"/>
  
  <owl:Class rdf:ID="AnnotatedUniProtRecord">
    <rdfs:subClassOf rdf:resource="http://purl.oclc.org/SADI/LSRN/UniProt_Record"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&benv;hasName"/>
        <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int">0</owl:minCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <!--
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&benv;hasDescription"/>
        <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int">1</owl:minCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    -->
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&benv;belongsToOrganism"/>
        <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int">1</owl:minCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  
</rdf:RDF>
