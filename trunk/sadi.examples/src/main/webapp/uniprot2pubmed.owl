<?xml version="1.0"?>
<!DOCTYPE rdf:RDF [
  <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
  <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
  <!ENTITY owl "http://www.w3.org/2002/07/owl#">
  <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
  <!ENTITY benv "http://sadiframework.org/ontologies/predicates.owl#">
  <!ENTITY lsrn "http://purl.oclc.org/SADI/LSRN/">
  <!ENTITY common "http://sadiframework.org/examples/common.owl#">
  <!ENTITY dumontier "http://ontology.dumontierlab.com/">
]>
<rdf:RDF
    xmlns:rdf="&rdf;"
    xmlns:rdfs="&rdfs;"
    xmlns:owl="&owl;"
    xmlns:xsd="&xsd;"
    xmlns:benv="&benv;"
    xmlns:lsrn="&lsrn;"
    xmlns:dumontier="&dumontier;"
    xmlns:common="&common;">

  <owl:Ontology rdf:about="">
    <owl:imports rdf:resource="&dumontier;"/>
  </owl:Ontology>
  
  <owl:Class rdf:ID="AnnotatedUniProtRecord">
    <rdfs:subClassOf rdf:resource="&lsrn;UniProt_Record"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&dumontier;hasReference"/>
        <owl:someValuesFrom rdf:resource="&lsrn;PubMed_Record"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  
</rdf:RDF>
